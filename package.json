{
  "name": "dc-sdk-box",
  "version": "0.0.3",
  "description": "DApp on dao casino protocol example",
  "author": "Alex Step",
  "license": "MIT",
  "meta": {
    "_": "This section data used for generate manifest.json and meta-tags html",
    "start_url": "./",
    "appName": "Dapp example",
    "appDescription": "Example DApp on dao casino protocol",
    "background": "#1c2026",
    "theme_color": "#1c2026",
    "display": "standalone",
    "orientation": "portrait"
  },
  "paths": {
    "dapp": {
      "contract_abi": "./dapp/config/dapp.contract.json"
    },
    "truffle": {
      "contracts_directory": "./contracts",
      "migrations_directory": "./migrations",
      "contracts_build_directory": "./dapp/config/contracts/"
    }
  },
  "scripts": {
    "postinstall": "sh ./scripts/postinstall.sh",
    "meta:generate": "cd ./scripts/tools/metainfo && npm i && npm start",
    "meta:clean": "rm -rf  ./dapp/static/meta",
    "watch_contracts": "node ./scripts/watch_contracts.js",
    "start": "sh ./scripts/start.sh && npm run dev",
    "dev": "node ./scripts/devserver.js",
    "stop": "sh ./scripts/stop.sh",
    "reset": "sh ./scripts/reset.sh",
    "build": "node ./scripts/build.js",
    "deploy:contracts:local": "truffle migrate --reset",
    "deploy:contracts:ropsten": "DC_NETWORK=ropsten truffle migrate --network ropsten",
    "deploy:frontend:ipfs": "cd ./scripts/tools/deploy_ipfs && npm run deploy",
    "deploy:frontend:firebase": "cd ./scripts/tools/deploy_firebase && yarn && npm run deploy",
    "deploy:frontend:ssh": "cd ./scripts/tools/deploy_ssh && yarn && npm run deploy",
    "publish": "echo 'Publish to dao.casino marketeplace - coming soon'",
    "logs:bankroller": "cd _env && npm run logs_bankroller",
    "logs:rpc": "cd _env && npm run logs_rpc",
    "lint": "eslint ./",
    "test": "truffle test"
  },
  "dependencies": {
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "ethereumjs-abi": "0.6.5",
    "pixi": "0.3.1",
    "web3": "1.0.0-beta.35",
    "bootstrap": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "reactstrap": "^6.5.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "6.22.2",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-env": "1.1.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "3.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "6.22.0",
    "babel-runtime": "6.26.0",
    "bootstrap": "^4.1.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.7",
    "dc-messaging": "github:DaoCasino/dc-messaging",
    "dexie": "^2.0.3",
    "dexie-observable": "^1.0.0-beta.4",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "^2.0.0",
    "extra-watch-webpack-plugin": "1.0.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "js-cryptico": "1.0.2",
    "json-loader": "^0.5.4",
    "object-assign": "4.1.1",
    "pixi.js": "^4.8.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.1.6",
    "promise": "8.0.1",
    "promise-worker": "^1.1.1",
    "raf": "3.4.0",
    "react": "^16.5.2",
    "react-dev-utils": "5",
    "react-dom": "^16.5.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.4",
    "reactstrap": "^6.5.0",
    "remove": "^0.1.5",
    "resolve": "1.6.0",
    "rollbar": "2.4.2",
    "sass-loader": "7.1.0",
    "serviceworker-webpack-plugin": "0.2.3",
    "style-loader": "0.19.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "truffle-contract": "^1.1.11",
    "truffle-hdwallet-provider-privkey": "0.3.0",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "2.0.3",
    "whatwg-fetch": "2.0.4",
    "worker-loader": "2.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "env",
      "stage-0"
    ]
  },
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "eslintConfig": {
    "extends": "react-app"
  }
}
